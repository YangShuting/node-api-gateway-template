{
  "name": "node-api-gateway-template",
  "version": "0.0.1",
  "scripts": {
    "_transduce": "./node_modules/.bin/babel src -d lib --presets=es2015 --ignore=*__tests__*",
    "_copy-static": "cp src/init/*.json lib/init/",
    "test": "jest && jasmine-node test",
    "build": "npm run _transduce && npm run _copy-static",
    "start": "node --debug lib/app.js"
  },
  "author": "@simonedesordi",
  "dependencies": {
    "babel": "6.*",
    "babel-cli": "*",
    "babel-core": "6.*",
    "babel-polyfill": "6.*",
    "babel-preset-es2015": "*",
    "babel-preset-stage-3": "*",
    "bcryptjs": "~2.0.2",
    "body-parser": "~1.0.0",
    "compression": "^1.0.11",
    "cors": "^2.4.1",
    "dateformat": "*",
    "debug": "~0.7.4",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "express-unless": "*",
    "falcor-express": "^0.1.2",
    "falcor-router": "^0.2.12",
    "jsonfile": "^2.2.3",
    "jsonwebtoken": "*",
    "lodash": "*",
    "minimist": "*",
    "mongoose": "*",
    "morgan": "*",
    "on-finished": "*",
    "ramda": "^0.19.1",
    "redis": "^2.4.2",
    "request-json": "*",
    "response-time": "*",
    "rx-node": "*",
    "winston": "*"
  },
  "devDependencies": {
    "babel-jest": "*",
    "frisby": "^0.8.5",
    "jest-cli": "*"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  }
}
